<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :song_genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :name, :name %>
  <%= f.submit %>
  <br>
  <input type=text name="song[note_ids][]" id="song_notes_1">
  <input type=text name="song[note_ids][]" id="song_notes_2">


<% end %>
